{
  "name": "mahmouds-portfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "watch:sass": "sass src/styles/sass/main.scss src/styles/css/style.css --watch",
    "dev:react": "vite",
    "dev": "npm-run-all --parallel dev:react watch:sass",
    "compile:sass": "sass src/styles/sass/main.scss src/styles/css/style.comp.css",
    "prefex:css": "postcss --use autoprefixer -b 'last 10 versions' src/styles/css/style.comp.css -o src/styles/css/style.prefix.css",
    "compress:css": "sass src/styles/css/style.prefix.css src/styles/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefex:css compress:css",
    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@phosphor-icons/react": "^2.1.7",
    "@reduxjs/toolkit": "^2.2.7",
    "leaflet": "^1.9.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-lazyload": "^3.2.1",
    "react-leaflet": "^4.2.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.26.0",
    "typed.js": "^2.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@tailwindcss/vite": "^4.0.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.3.4",
    "aos": "^2.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^11.0.0",
    "prop-types": "^15.8.1",
    "sass": "^1.83.0",
    "tailwindcss": "^4.0.1",
    "vite": "^6.0.5"
  }
}
